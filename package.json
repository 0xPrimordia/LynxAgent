{
  "name": "lynx-agent-runner",
  "version": "1.0.0",
  "description": "Wrapper to run the Lynx Agent from the standards-agent-kit",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "cd standards-agent-kit && npm install",
    "start": "cd standards-agent-kit && npm run lynx-agent:start",
    "start-sentinel": "cd standards-agent-kit && npm run lynx-agent:start-sentinel",
    "start-rebalancer": "cd standards-agent-kit && npm run lynx-agent:start-rebalancer",
    "start-governance": "cd standards-agent-kit && npm run lynx-agent:start-governance",
    "register-sentinel": "cd standards-agent-kit && npm run lynx-agent:register-sentinel",
    "register-rebalancer": "cd standards-agent-kit && npm run lynx-agent:register-rebalancer",
    "register-governance": "cd standards-agent-kit && npm run lynx-agent:register-governance",
    "connection-test": "cd standards-agent-kit && npm run lynx-agent:connection-test",
    "send-message": "cd standards-agent-kit && npm run lynx-agent:send-message",
    "listen": "cd standards-agent-kit && npm run lynx-agent:listen",
    "test-all": "cd standards-agent-kit && npm run lynx-agent:test-all",
    "lynx-agent:send-test-vote": "cd standards-agent-kit/examples/lynx-agent && tsx send-test-vote.ts",
    "lynx-agent:check-contract": "cd standards-agent-kit/examples/lynx-agent && tsx check-contract-ratios.ts",
    "lynx-agent:test-contract-execution": "cd standards-agent-kit/examples/lynx-agent && tsx test-contract-execution.ts",
    "lynx-agent:run-contract-test": "cd standards-agent-kit/examples/lynx-agent && tsx run-contract-test.ts",
    "lynx-agent:check-logs": "cd standards-agent-kit/examples/lynx-agent && tsx check-governance-logs.ts status",
    "lynx-agent:monitor-logs": "cd standards-agent-kit/examples/lynx-agent && tsx check-governance-logs.ts monitor",
    "lynx-agent:diagnose": "cd standards-agent-kit/examples/lynx-agent && tsx diagnose-governance.ts",
    "lynx-agent:run-local": "cd standards-agent-kit/examples/lynx-agent && tsx run-local-governance.ts",
    "lynx-agent:test-local": "cd standards-agent-kit/examples/lynx-agent && tsx test-local-contract.ts",
    "postinstall": "cd standards-agent-kit && npm install"
  },
  "dependencies": {},
  "private": true
} 